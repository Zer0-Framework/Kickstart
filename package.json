  "scripts": {
    "bundle": "npm run browserify; npm run bundle-minify-js",
    "bundle-dev": "npm run browserify-dev",
    "bundle-minify-js": "minifyjs -m -i dist/$BUNDLE.bundle.js -o dist/$BUNDLE.bundle.min.js",
    "browserify": "browserify -t [ browserify-css --minify=true --output dist/$BUNDLE.bundle.min.css ] -g uglifyify -g aliasify public/js/$BUNDLE.js -o dist/$BUNDLE.bundle.js --debug",
    "browserify-dev": "browserify -t [ browserify-css --output dist/$BUNDLE.bundle.css ] -g aliasify public/js/$BUNDLE.js -o dist/$BUNDLE.bundle.js --debug",
    "watch": "watchify -t [ browserify-css --output dist/$BUNDLE.bundle.css ] -g aliasify public/js/$BUNDLE.js -o dist/$BUNDLE.bundle.js -v --debug",
    "spritify": "spritify -c spritify.json",
    "prepare": "patch-package"
  },
  "browserify-css": {
    "autoInject": false,
    "rootDir": "."
